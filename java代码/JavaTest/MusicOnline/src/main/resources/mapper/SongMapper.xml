<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beiyuan.music.mapper.SongMapper">
    <update id="updateCover" >
        update song set cover =#{uploadUrl} where id=#{id}
    </update>

    <update id="updateUrl" >
        update song set url =#{uploadUrl} where id=#{id}
    </update>
</mapper>
